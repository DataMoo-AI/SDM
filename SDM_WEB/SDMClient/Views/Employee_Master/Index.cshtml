    function Gridappend(data)
    {
        
        var item = '';
        var Arr;
        if (data!=undefined)
        {
            if (data.length > 0)
            {
                Arr = data;
                for (var i = 0; i < Arr.length; i++)
                {
                    var sno=i+1;
                    item += '<tr>';
                    item += '<td>' + sno+ '</td>';
                    item += '<td>' + Arr[i].empName + '</td>';
                    item += '<td>' + Arr[i].empJobLocation + '</td>';
                    item += '<td>' + Arr[i].empCode + '</td>';
                    item += '<td>' + Arr[i].empCostCenterName + '</td>';
                    item += '  <td>';
                    item += ActionsButtons(Arr[i].empId)
                    item += '</td>';
                    item += ' </tr>';
                }
                $('#bodyData').html('').append(item);

            }
            else
            {
                $('#bodyData').html('').append('<tr><td colspan="10" style="text-align:center">No records found.</td></tr>');
            }
        }
        else
        {
            $('#bodyData').html('').append('<tr><td colspan="10" style="text-align:center">No records found.</td></tr>');
        }
    }
