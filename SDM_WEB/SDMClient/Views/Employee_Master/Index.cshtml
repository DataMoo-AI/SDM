    async function GridShow(Flag)
    {
        if(Flag==1)
        {
            $('.Gridtable').show();
            $('.GridIUD').hide();
            $('.GridView').hide();
        }
        else if(Flag==2)
        {
            $('.Gridtable').hide();
            $('.GridIUD').show();
            $('.GridView').hide();
            $('#ddlCostCenter').find('option').remove().end().append('<option value="0">Select</option>');
            await BindCostCenter();
        }
        else
        {
            $('.Gridtable').hide();
            $('.GridIUD').hide();
            $('.GridView').show();
            $('#ddlCostCenter').find('option').remove().end().append('<option value="0">Select</option>');
            await BindCostCenter();
        }
    }
