    function ImagesBind(Data,IDS){
        var rows ="";
        try{
        var urls="@AppHostName";
        for(var i=0;i<Data.length;i++){
            rows +='<a target="_blank"  href="'+urls+Data[i]+'"> <img  height="75px" src="'+urls+Data[i]+'"/> </a> ';
        }
        }catch{}
        $('#'+IDS).html('').append(rows);
        $('#'+IDS+'_').html('').append(rows);

    }
