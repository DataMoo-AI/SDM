@using Newtonsoft.Json;
@using SDM.Common.Response;
@{
    var _response = JsonConvert.DeserializeObject<Response>(Context.Session.GetString("_response"));
    var Bew = "";
}

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>SDM Office</title>

  <!-- Google Font: Source Sans Pro -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
  <!-- Font Awesome --> 
  <link rel="stylesheet" href="/theme/plugins/fontawesome-free/css/all.min.css">
  <!-- DataTables -->
  <link rel="stylesheet" href="/theme/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
  <link rel="stylesheet" href="/theme/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
  <link rel="stylesheet" href="/theme/plugins/datatables-buttons/css/buttons.bootstrap4.min.css">
  <!-- Theme style -->
    <link href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" rel="stylesheet" />
      <link rel="stylesheet" href="/theme/dist/css/adminlte.min.css"> 
      <!-- Select2 -->
  <link rel="stylesheet" href="/theme/plugins/select2/css/select2.min.css">
  <link rel="stylesheet" href="/theme/plugins/select2-bootstrap4-theme/select2-bootstrap4.min.css">
  <style>
table td,table th{
border:none !important;
border-top:0px !important;
}
</style>
 </head>
 <body>

     <div>
         <div class="p-3 mb-3">
              <!-- title row -->
              <div class="row">
                <div class="col-12">
                  <h4>
                    <i class="fas fa-globe"></i> SDM.
                    <small class="float-right">Date: @DateTime.Now</small>
                  </h4>
                </div>
                <!-- /.col -->
              </div>
              <!-- info row -->
              <div class="row invoice-info">
                <div class="col-sm-4 invoice-col">
                  From
                  <address>
                    <strong>SDM Office,.</strong><br>
                    Dubai<br> 
                    Phone: (123) 123-123<br>
                    Email: info@sdm.com
                  </address>
                </div> 
              </div>
              <!-- /.row -->
 

              <div class="row">
           
                <div class="col-12">
                       <div class="row col-md-12">
                  <h3 class="text-center">Vehicle</h3>
</div>
                  <div class="table-responsive">
                    <table border="0" class="table"width="100%">
                      <tbody>
                          <tr>
                        <th style="width:25%">Cost Center:</th>
                        <td>@_response.VechileResponse[0].VhCostCentreName</td>
                        
                        <th style="width:25%">Code:</th>
                        <td>@_response.VechileResponse[0].VhCode</td>
                      </tr>
                           <tr>
                        <th style="width:25%">Type:</th>
                        <td>@_response.VechileResponse[0].VhType</td>
                        
                        <th style="width:25%">Registration Number:</th>
                        <td>@_response.VechileResponse[0].VhRegNo</td>
                      </tr>
                      <tr>
                      <th style="width:25%"> Name:</th>
                        <td>@_response.VechileResponse[0].VhName</td>

                        <th style="width:25%">Brand Name:</th>
                        <td>@_response.VechileResponse[0].VhMakeBrand</td>
                      </tr>

                      <tr>
                      <th style="width:25%"> Model Year:</th>
                        <td>@_response.VechileResponse[0].VhModelYear</td>

                        <th style="width:25%">Purchase Date:</th>
                        <td>@_response.VechileResponse[0].VhPurchaseDt</td>
                      </tr>

                      <tr>
                      <th style="width:25%"> Vehicle On:</th>
                        <td>@_response.VechileResponse[0].VhVehicleOn</td>

                        <th style="width:25%">Registraion Expire:</th>
                        <td>@_response.VechileResponse[0].VhRegistrationExp</td>
                      </tr>

                      <tr>
                      <th style="width:25%"> Type Of Purchase :</th>
                        <td>@_response.VechileResponse[0].VhTypeOfPurchase</td>

                        <th style="width:25%">Size Capacity:</th>
                        <td>@_response.VechileResponse[0].VhSizeCapacity</td>
                      </tr>

                      <tr>
                      <th style="width:25%"> Driven By:</th>
                        <td>@_response.VechileResponse[0].VhDrivenBy</td>

                        <th style="width:25%">Location :</th>
                        <td>@_response.VechileResponse[0].VhLocation</td>
                      </tr>
                      
                      <tr>
                      <th style="width:25%">  Purchase Value:</th>
                        <td>@_response.VechileResponse[0].VhPurchaseValue</td>

                        <th style="width:25%">Purchase From:</th>
                        <td>@_response.VechileResponse[0].VhPurchasedFrom</td>
                      </tr>

                      <tr>
                      <th style="width:25%"> Purchase Colour:</th>
                        <td>@_response.VechileResponse[0].VhPurchasedColor</td>

                        <th style="width:25%">Vehicle Colour:</th>
                        <td>@_response.VechileResponse[0].VhColour</td>
                      </tr>


                      <tr>
                      <th style="width:25%"> Engine Power:</th>
                        <td>@_response.VechileResponse[0].VhEnginePower</td>

                        <th style="width:25%">Number Of Engine:</th>
                        <td>@_response.VechileResponse[0].VhNoOfEngines</td>
                      </tr>

                      <tr>
                      <th style="width:25%"> Engine Serial No:</th>
                        <td>@_response.VechileResponse[0].VhEngineSerialNumber</td>

                        <th style="width:25%">Engine Make:</th>
                        <td>@_response.VechileResponse[0].VhEngineMake</td>
                      </tr>

                      <tr>
                      <th style="width:25%"> Type Of Hull:</th>
                        <td>@_response.VechileResponse[0].VhTypeOfHull</td>

                        <th style="width:25%">Hull Chassis No:</th>
                        <td>@_response.VechileResponse[0].VhHullChassisNo</td>
                      </tr>

                      <tr>
                      <th style="width:25%"> Weight:</th>
                        <td>@_response.VechileResponse[0].VhWeight</td>

                        <th style="width:25%">Origin Place:</th>
                        <td>@_response.VechileResponse[0].VhOriginPlace</td>
                      </tr>

                      <tr>
                      <th style="width:25%"> No Of Passengers:</th>
                        <td>@_response.VechileResponse[0].VhNoOfPassengers</td>

                        <th style="width:25%">Number Of Crew:</th>
                        <td>@_response.VechileResponse[0].VhNoOfCrew</td>
                      </tr>

                      <tr>
                      <th style="width:25%"> Radio Device Name:</th>
                        <td>@_response.VechileResponse[0].VhRadioDeviceName</td>

                        <th style="width:25%">Radio Device S.No:</th>
                        <td>@_response.VechileResponse[0].VhRadioDeviceSno</td>
                      </tr>

                      <tr>
                      <th style="width:25%"> Radio Type:</th>
                        <td>@_response.VechileResponse[0].VhRadioDeviceType</td>

                        <th style="width:25%">Photo:</th>
                        <td><img   src=" @_response.VechileResponse[0].VhImage" height="50px"/></td>
                      </tr>

                      <tr>
                      <th style="width:25%"> Status:</th>
                        <td>@_response.VechileResponse[0].VhStatus</td>

                        <th style="width:25%">Others1:</th>
                        <td>@_response.VechileResponse[0].VhOthers1</td>
                      </tr>

                      <tr>
                      <th style="width:25%"> Others2:</th>
                        <td>@_response.VechileResponse[0].VhOthers2</td>

                        <th style="width:25%">Others3:</th>
                        <td>@_response.VechileResponse[0].VhOthers3</td>
                      </tr>

                      <tr>
                      <th style="width:25%"> Others4:</th>
                        <td>@_response.VechileResponse[0].VhOthers4</td>
                    </tr>

                    </tbody></table>
                  </div>
                </div>
                <!-- /.col -->
              </div>
              <!-- /.row -->

              <!-- this row will not appear when printing -->
            
              </div>
            </div>

            <div class="text-center">
                Thank you
                  </div>

     </div>



<!-- jQuery -->
<script src="/theme/plugins/jquery/jquery.min.js"></script>
<!-- Select2 -->
<script src="/theme/plugins/select2/js/select2.full.min.js"></script>
