@using Newtonsoft.Json;
@using SDM.Common.Response;
@{
    var _response = JsonConvert.DeserializeObject<Response>(Context.Session.GetString("_response"));
    var _Headingresponse = JsonConvert.DeserializeObject<Response>(Context.Session.GetString("_Headingresponse"));

    var Bew = "";
}

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>SDM Office</title>

  <!-- Google Font: Source Sans Pro -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
  <!-- Font Awesome --> 
  <link rel="stylesheet" href="~/theme/plugins/fontawesome-free/css/all.min.css">
  <!-- DataTables -->
  <link rel="stylesheet" href="~/theme/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
  <link rel="stylesheet" href="~/theme/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
  <link rel="stylesheet" href="~/theme/plugins/datatables-buttons/css/buttons.bootstrap4.min.css">
  <!-- Theme style -->
    <link href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" rel="stylesheet" />
      <link rel="stylesheet" href="~/theme/dist/css/adminlte.min.css"> 
      <!-- Select2 -->
  <link rel="stylesheet" href="~/theme/plugins/select2/css/select2.min.css">
  <link rel="stylesheet" href="~/theme/plugins/select2-bootstrap4-theme/select2-bootstrap4.min.css">
    <style>
table td,table th{
border:none !important;
border-top:0px !important; 
}
</style>
 </head>
 <body>

     <div>
         <div class="p-3 mb-3">
              <!-- title row -->
                <!-- title row -->
             <div class="row">
                <div class="col-12">
                  <h4>
                   @* <i class="fas fa-globe"></i> @_response.CustomerMasterResponse[0].CustName*@
                    <small class="float-right">Date: @DateTime.Now</small>
                  </h4>
                </div>
                <!-- /.col -->
              </div>
              <!-- info row -->
              <div class="row invoice-info">
                <div class="col-sm-4 invoice-col">
                   
                  <address>
                    <strong>@_Headingresponse.CustomerMasterResponse[0].CustAddress,.</strong><br> 
                    Phone: @_Headingresponse.CustomerMasterResponse[0].CustContactMobile<br>
                    Email: @_Headingresponse.CustomerMasterResponse[0].CustEmail
                  </address>
                </div> 
              </div>
              <!-- /.row -->
              <!-- /.row -->
 

              <div class="row">
           
                <div class="col-12">
                <div class="row col-md-12">
                  <h3 class="text-center">Bank</h3>
</div>

                  <div class="table-responsive">
                    <table class="table"width="100%">
                      <tbody>
                          <tr>
                        <th style="width:25%">Name:</th>
                        <td>@_response.BankResponse[0].BnkName</td>
                        
                        <th style="width:25%">Code:</th>
                        <td>@_response.BankResponse[0].BnkCode</td>
                      </tr>
                           <tr>
                        <th style="width:25%">Account Name:</th>
                        <td>@_response.BankResponse[0].BnkAccName</td>
                        
                        <th style="width:25%">Account Number:</th>
                        <td>@_response.BankResponse[0].BnkAccNo</td>
                      </tr>
                      <tr>
                       <th style="width:25%">Credit/Debit:</th>
                        <td>@_response.BankResponse[0].BnkCreditOrDebit</td>

                       <th style="width:25%">Credit/Debit2 :</th>
                        <td>@_response.BankResponse[0].BnkCreditOrDebit2</td>

                      </tr> 
                      <tr>
                      <th style="width:25%">Credit/Debit3:</th>
                        <td>@_response.BankResponse[0].BnkCreditOrDebit3</td>

                         <th style="width:25%">Credit/Debit4:</th>
                        <td>@_response.BankResponse[0].BnkCreditOrDebit4</td>
                      </tr>
                      <tr>
                       <th style="width:25%">Crdit/Debit5:</th>
                        <td>@_response.BankResponse[0].BnkCreditOrDebit5</td>

                         <th style="width:25%">Currency:</th>
                                    @if (@_response.BankResponse[0].BnkAccountCcy == 1)
                                    {
                                         <td>AED</td>
                                    }
                                     @if (@_response.BankResponse[0].BnkAccountCcy == 2)
                                    {
                                        <td>USA</td>
                                    }
                                    else
                                    {
                                         <td></td>
                                    }

                      </tr>
                      <tr>
                           <th style="width:25%">Swift Code:</th>
                        <td>@_response.BankResponse[0].BnkSwiftCode</td>

                         <th style="width:25%">Address:</th>
                        <td>@_response.BankResponse[0].BnkAddress</td>
                      </tr>
                     
                      <tr>
                           <th style="width:25%">Others :</th>
                        <td>@_response.BankResponse[0].BnkOthers1</td>

                         <th style="width:25%">IBAN Number:</th>
                        <td>@_response.BankResponse[0].BnkIbanNumb</td>
                      </tr>
                      <tr>
                           <th style="width:25%">RM Name:</th>
                        <td>@_response.BankResponse[0].RmName</td>

                         <th style="width:25%">RM Contact Number:</th>
                        <td>@_response.BankResponse[0].RmContact</td>
                      </tr>
                      <tr>
                           <th style="width:25%">RM Email:</th>
                        <td>@_response.BankResponse[0].RmEmail</td>

                         <th style="width:25%">RM1 Nmae:</th>
                        <td>@_response.BankResponse[0].Rm1Name</td>
                      </tr>
                      <tr>
                           <th style="width:25%">RM1 Contact Number:</th>
                        <td>@_response.BankResponse[0].Rm1Contact</td>
                      
                         <th style="width:25%">RM1 Email:</th>
                        <td>@_response.BankResponse[0].Rm1Email</td>

                      </tr>
                      <tr>
                           <th style="width:25%">RM2 Name:</th>
                        <td>@_response.BankResponse[0].Rm2Name</td>

                         <th style="width:25%">Rm2 Contact Number:</th>
                        <td>@_response.BankResponse[0].Rm2Contact</td>
                      </tr>
                      <tr>
                         <th style="width:25%">Rm2 Email:</th>
                         <td>@_response.BankResponse[0].Rm2Email</td>

                      </tr>
                      


                    </tbody></table>
                  </div>
                </div>
                <!-- /.col -->
              </div>
              <!-- /.row -->

              <!-- this row will not appear when printing -->
            
              </div>
            </div>

            <div class="text-center">
                Thank you
                  </div>

     </div>



<!-- jQuery -->
<script src="~/theme/plugins/jquery/jquery.min.js"></script>
<!-- Select2 -->
<script src="~/theme/plugins/select2/js/select2.full.min.js"></script>
<!-- Bootstrap 4 -->
<script src="~/theme/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
<!-- DataTables  & Plugins -->
<script src="~/theme/plugins/datatables/jquery.dataTables.min.js"></script>
<script src="~/theme/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
<script src="~/theme/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
<script src="~/theme/plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
<script src="~/theme/plugins/datatables-buttons/js/dataTables.buttons.min.js"></script>
<script src="~/theme/plugins/datatables-buttons/js/buttons.bootstrap4.min.js"></script>
<script src="~/theme/plugins/jszip/jszip.min.js"></script>
<script src="~/theme/plugins/pdfmake/pdfmake.min.js"></script>
<script src="~/theme/plugins/pdfmake/vfs_fonts.js"></script>
<script src="~/theme/plugins/datatables-buttons/js/buttons.html5.min.js"></script>
<script src="~/theme/plugins/datatables-buttons/js/buttons.print.min.js"></script>
<script src="~/theme/plugins/datatables-buttons/js/buttons.colVis.min.js"></script>
<!-- AdminLTE App -->
<script src="~/theme/dist/js/adminlte.min.js"></script> 
<!-- Page specific script -->
<script>
    

  $(function () {
    $("#example1").DataTable({
      "responsive": true, "lengthChange": false, "autoWidth": false,
      "buttons": ["copy", "csv", "excel", "pdf", "print", "colvis"]
    }).buttons().container().appendTo('#example1_wrapper .col-md-6:eq(0)');
    $('#example2').DataTable({
      "paging": true,
      "lengthChange": false,
      "searching": true,
      "ordering": true,
      "info": true,
      "autoWidth": false,
      "responsive": true,
      "columnDefs": [
                {
                    'searchable': false,
                    'targets': [0]
                },
            ],
    });
     $('.select2').select2()
  });
  
        $(window).on("load", function () {
            $("#pageloader").fadeOut("slow");
        });
</script>
<style>
.form-row-field-input:focus~label, input:not(:placeholder-shown) ~label {
    top: -5px;
    font-size: 12px;
    color: #000 !important;
    font-weight: bold;
}</style>
</body>
</html>